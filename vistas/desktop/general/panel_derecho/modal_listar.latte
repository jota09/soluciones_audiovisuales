      </div>
      <div class="modal-body">
        <div class="clearfix"></div>
        <table id="tabla_listar_cotizaciones" data-modulo="cotizaciones" class="display cell-border">
          <thead>
            <tr>
              <th>id</th>
              <th>Número Factura</th>
              {if condition="  $modulo !== 'cuentas' && $modulo !== 'oportunidades'"}
